<p id="notice"><%= notice %></p>

<h1>Listing Kucings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @kucings.each do |kucing| %>
      <tr>
        <td><%= link_to 'Show', kucing %></td>
        <td><%= link_to 'Edit', edit_kucing_path(kucing) %></td>
        <td><%= link_to 'Destroy', kucing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Kucing', new_kucing_path %>
